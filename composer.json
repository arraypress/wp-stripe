{
  "name": "arraypress/wp-stripe",
  "version": "1.0.0",
  "description": "A comprehensive WordPress library for working with the Stripe API, including client management, webhooks, products, prices, coupons, checkout sessions, and utilities.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-stripe",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=8.1",
    "stripe/stripe-php": "^19.0",
    "arraypress/wp-currencies": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Stripe\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-stripe/issues",
    "source": "https://github.com/arraypress/wp-stripe"
  },
  "keywords": [
    "wordpress",
    "wp",
    "stripe",
    "payments",
    "checkout",
    "webhooks",
    "subscriptions",
    "arraypress"
  ]
}
